 @model DepartmentVM
@{
    ViewData["Title"] = "Details";
}

<div class="block-header">
    <h2 >Details For @Model.Name Department</h2>
</div>



 <div class="form-group">
    <label asp-for="Name">DepartName:</label>
    <input type="text" class="form-control" asp-for="Name" autocomplete="off" readonly >
  </div>
  <div class="form-group">
    <label asp-for="Code">Code:</label>
    <input type="text" class="form-control" asp-for="Code" autocomplete="off" readonly>
  </div>
  <center >
                  <a asp-action="index" asp-controller="Department" class="btn btn-primary">Return</a>
                  <a asp-action="Update" asp-controller="Department" asp-route-id="@Model.Id" class="btn btn-warning">Update</a>
                  <a asp-action="Delete" asp-controller="Department" asp-route-id="@Model.Id" class="btn btn-danger">Delete</a>

  </center>