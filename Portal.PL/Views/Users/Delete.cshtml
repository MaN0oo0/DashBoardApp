
@{
    ViewData["Title"] = "Delete";
}
@model ApplicationUser

@section Styles{
    <link href="~/plugins/bootstrap-select/css/bootstrap-select.css" rel="stylesheet" />
}


<div>
    <h2 class="badge rounded-pill bg-secondary p-2 mt-2"> @SharedLocalizer["AreyousuretoDelete"] : @Model.UserName  </h2>
</div>



<form asp-controller="Users" asp-action="Delete" class="form-group">



    <input type="hidden" asp-for="Id" />
    <input type="hidden" asp-for="SecurityStamp" />
        <input type="hidden" asp-for="ConcurrencyStamp" />

  



    <center class="mt-5 p-5" style="margin:60px auto">
        <input type="submit" value=" @SharedLocalizer["Delete"]" class="btn btn-danger" />
        <a asp-controller="Users" asp-action="Index" class="btn btn-primary"> @SharedLocalizer["Return"]</a>

    </center>

</form>

